{
    "name": "larahook/distinct-on-pagination",
    "type": "library",
    "description": "ORM Multiple distinct() with paginate()",
    "keywords": [
        "laravel",
        "pagination",
        "multiple",
        "distinct on",
        "orm",
        "total count"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mikhail Mishankin",
            "email": "mishanki@yandex.ru"
        }
    ],
    "homepage": "https://github.com/Mishanki/laravel-distincton-pagination",
    "support": {
        "email": "mishanki@yandex.ru",
        "issues": "https://github.com/Mishanki/laravel-distincton-pagination/issues",
        "source": "https://github.com/Mishanki/laravel-distincton-pagination"
    },
    "require": {
        "php": "^8.0",
        "laravel/framework": "^10.0|^11.0",
        "marcocesarato/php-conventional-changelog": "^1.17"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.51"
    },
    "autoload": {
        "psr-4": {
            "Larahook\\DistinctOnPagination\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larahook\\DistinctOnPagination\\DistinctOnPaginationServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "1.1.3"
}